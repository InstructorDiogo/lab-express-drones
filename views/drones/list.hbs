<h2>Available drones</h2>

<hr>
<hr>

{{#each drones}}

<div>
    <h2>Name:</h2>
    <h3>{{name}}</h3>
    <h2>Propellers:</h2>
    <h3>{{propellers}}</h3>
    <h2>MaxSpeed:</h2>
    <h3>{{maxSpeed}}</h3>

    <a href="/drones/{{_id}}/edit">Edit</a>
    <form action="/drones/{{_id}}/delete" method="POST">
        <button>Delete</button>
    </form>

</div>

<hr>

{{/each}}
